{
  "name": "svedex-ui",
  "version": "0.1.0",
  "description": "A simple starter project to get up and developing quickly with the blend of Svelte and Electron JS for building a Native Desktop App on various OS Platforms like Windows, Linux and macOS using Electron Builder.",
  "main": "main.js",
  "author": "Souleh Shaikh <author.souleh@thegeekytutor.com>",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/soulehshaikh99/create-svelte-electron-app"
  },
  "bugs": {
    "url": "https://github.com/soulehshaikh99/create-svelte-electron-app/issues"
  },
  "build": {
    "icon": "public/favicon.png",
    "productName": "Svelte and Electron App",
    "files": [
      "public/**/*",
      "main.js"
    ],
    "win": {},
    "linux": {},
    "mac": {}
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "electron": "wait-on http://localhost:5000 && electron .",
    "electron-dev": "concurrently \"npm run dev\" \"npm run electron\"",
    "preelectron-pack": "yarn run build",
    "electron-pack": "electron-builder"
  },
  "dependencies": {
    "electron-serve": "^1.0.0",
    "augmented-ui": "^1.1.2",
    "color": "3.1.3",
    "geolite2-redist": "^2.0.2",
    "howler": "2.2.1",
    "maxmind": "4.3.1",
    "mime-types": "^2.1.28",
    "nanoid": "3.1.20",
    "node-pty": "0.9.0",
    "pdfjs-dist": "2.7.570",
    "pretty-bytes": "5.5.0",
    "shell-env": "3.0.1",
    "signale": "1.4.0",
    "smoothie": "1.35.0",
    "systeminformation": "4.34.9",
    "tail": "2.2.0",
    "username": "5.1.0",
    "which": "2.0.2",
    "ws": "7.4.2",
    "xterm": "4.10.0",
    "xterm-addon-attach": "0.6.0",
    "xterm-addon-fit": "0.5.0",
    "xterm-addon-ligatures": "0.4.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "concurrently": "^5.3.0",
    "electron": "^11.2.3",
    "electron-builder": "^22.8.0",
    "electron-rebuild": "^2.3.5",
    "rollup": "^2.3.4",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^6.0.0",
    "sirv-cli": "^1.0.0",
    "svelte": "^3.0.0",
    "wait-on": "^5.2.0"
  },
  "keywords": [
    "cross-platform",
    "native-app",
    "installed-app",
    "desktop-applications",
    "windows",
    "linux",
    "macos",
    "html",
    "css",
    "javascript",
    "front-end",
    "compiler",
    "svelte",
    "rollup",
    "electron",
    "electron-builder",
    "electron-serve",
    "concurrently",
    "wait-on",
    "webapp"
  ]
}
